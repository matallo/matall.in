{
  "name": "matall.in",
  "version": "2.0.1",
  "description": "Personal website of Carlos Matallín",
  "main": "index.js",
  "scripts": {
    "build": "gulp",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "yarn run eslint *.js app/_js/{**/,}*.js __tests__/spec/{**/,}*.js",
    "lint:css": "yarn run stylelint app/_scss/{**/,}*.scss",
    "format": "yarn run prettier-eslint --prettierLast --write *.{js,json,md} app/_js/{**/,}*.js __tests__/spec/{**/,}*.js app/_scss/{**/,}*.scss",
    "start": "gulp serve",
    "test": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/_js/{,components/}*.js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matallo/matall.in.git"
  },
  "author": "Carlos Matallín",
  "license": "ISC",
  "homepage": "https://github.com/matallo/matall.in",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "critical": "^1.3.4",
    "css-loader": "^3.0.0",
    "d3-geo": "^1.11.6",
    "d3-selection": "^1.3.2",
    "del": "^5.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-download": "^0.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-load-plugins": "^2.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.4",
    "gulp-uglify": "^3.0.1",
    "html5shiv": "^3.7.3",
    "intersection-observer": "^0.7.0",
    "jest": "^24.1.0",
    "lazysizes": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "picturefill": "^3.0.3",
    "plugin-error": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^1.11.1",
    "sass-loader": "^8.0.0",
    "scrollama": "^2.0.0",
    "smoothscroll": "^0.4.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-property-sort-order-smacss": "^5.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
