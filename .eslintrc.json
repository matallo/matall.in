  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "double"
